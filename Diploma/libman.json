{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "vanta@0.5.24",
      "destination": "wwwroot/vanta/"
    },
    {
      "provider": "cdnjs",
      "library": "datatable@2.0.2",
      "destination": "wwwroot/lib/datatable/"
    },
    {
      "provider": "cdnjs",
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/"
    },
    {
      "provider": "cdnjs",
      "library": "Chart.js@4.3.0",
      "destination": "wwwroot/lib/Chart.js/"
    },
    {
      "provider": "cdnjs",
      "library": "select2@4.0.13",
      "destination": "wwwroot/lib/select2/"
    },
    {
      "provider": "cdnjs",
      "library": "select2-bootstrap-5-theme@1.3.0",
      "destination": "wwwroot/lib/select2-bootstrap-5-theme/"
    },
    {
      "library": "bs5-toast@1.0.0",
      "destination": "wwwroot/lib/bs5-toast/"
    }
  ]
}